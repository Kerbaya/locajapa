SELECT l.id, le
FROM ${entityName} l
JOIN l.localized le
WHERE
	l.id IN (:idSet)
	AND le.languageTag IN (:languageTagSet)
	AND le.languageLevel = (
		SELECT MAX(le2.languageLevel)
		FROM ${entityName} l2
		JOIN l2.localized le2
		WHERE
			l2.id = l.id
			AND le2.languageTag IN (:languageTagSet)
	)